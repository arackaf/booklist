{
  "name": "booklist",
  "main": "app.js",
  "description": "A book management web app for exploring modern web technologies.  Think ToDo, but more realistic",
  "version": "0.1.0",
  "scripts": {
    "start": "vite",
    "dev-all": "concurrently npm:dev npm:dev-sw --prefixColors blue,green",
    "dev-sw": "vite build --watch --config vite-service-worker.config.ts",
    "build": "vite build",
    "build-sw": "vite build --config vite-service-worker.config.ts",
    "tsc": "svelte-check --threshold error && tsc --noEmit",
    "svelte-check": "svelte-check --watch --threshold error",
    "prod-build": "npm run tsc && npm run build-sw && npm run build",
    "tscw": "tsc -w --noEmit",
    "persistgraphql": "persistgraphql gql"
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@types/node": "^16.11.10",
    "blurhash": "^1.1.5",
    "classnames": "2.2.5",
    "d3-array": "1.2.0",
    "d3-axis": "1.0.8",
    "d3-scale": "4.0.2",
    "d3-selection": "1.1.0",
    "d3-transition": "3.0.1",
    "dotenv": "^10.0.0",
    "history": "^5.0.0",
    "lodash.debounce": "4.0.8",
    "micro-graphql-svelte": "0.2.4",
    "node-sass": "7.0.1",
    "persistgraphql": "^0.3.11",
    "purify-css": "1.2.2",
    "query-string": "4.3.2",
    "sass": "^1.43.4",
    "shallow-equal": "^1.0.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.2.10",
    "svelte-file-dropzone": "0.0.15",
    "svelte-helpers": "0.2.5",
    "svelte-loadable": "^1.5.1",
    "svelte-preprocess": "^4.9.8",
    "svelte-toggle": "^3.1.0",
    "toastify-js": "^1.11.1",
    "typescript": "^4.5.2",
    "vite": "^2.7.1",
    "vite-plugin-pwa": "^0.11.7"
  },
  "devDependencies": {
    "prettier-plugin-svelte": "^1.4.1"
  },
  "workspaces": [
    "app",
    "gql",
    "modules",
    "styles",
    "util",
    "util/jscolor",
    "../booklist-ui"
  ]
}
