    "dotenv": "^4.0.0",
    "easy-express-controllers": "0.2.1",
    "es6-symbol": "2.0.1",
    "exif-parser": "0.1.9",
    "express": "4.x",
    "express-session": "1.12.1",
    "express-ws": "1.0.0",
    "lwip": "0.0.9",
    "mkdirp": "0.5.1",
    "moment": "^2.13.0",
    "mongodb": "2.0.39",
    "multer": "1.1.0",
    "nodemailer": "2.3.0",
    "nodemailer-smtp-transport": "2.4.1",
