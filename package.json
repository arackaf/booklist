{
  "name": "booklist",
  "main": "app.js",
  "engines": {
    "node": "8.11.1",
    "npm": "6.4.1"
  },
  "description": "A book management web app for exploring modern web technologies.  Think ToDo, but more realistic",
  "version": "0.1.0",
  "scripts": {
    "deploy": "npm run build && modulus deploy -p my-library",
    "build": "npm run react-redux-build",
    "react-redux-build": "cd react-redux && rm -rf dist && set NODE_ENV='production' && webpack && cd sw-manual && rollup -c",
    "start": "node app.js",
    "startw": "nodemon app.js",
    "heroku-postbuild": "cd react-redux && npm i && webpack && cd sw-manual && rollup -c"
  },
  "dependencies": {
    "apac": "3.0.2",
    "aws-sdk": "2.3.14",
    "blueimp-md5": "2.3.0",
    "body-parser": "1.13.2",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.0",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "easy-express-controllers": "1.0.2",
    "esm": "^3.0.84",
    "express": "4.x",
    "express-graphql": "0.6.11",
    "express-session": "1.12.1",
    "express-ws": "1.0.0",
    "generic-persistgraphql": "0.0.3",
    "glob": "7.0.3",
    "graphql": "14.0.2",
    "graphql-tools": "2.8.0",
    "jimp": "0.2.28",
    "lodash.flatmap": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "mkdirp": "0.5.1",
    "moment": "^2.13.0",
    "mongo-graphql-starter": "^0.9.0",
    "mongodb": "3.1.13",
    "multer": "1.1.0",
    "nodemailer": "2.3.0",
    "nodemailer-smtp-transport": "2.4.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-remember-me": "0.0.1",
    "request": "2.83.0",
    "rollup": "^0.67.3",
    "rollup-plugin-babel-minify": "^6.2.0",
    "rollup-plugin-uglify": "^6.0.0",
    "typescript": "^3.0.1",
    "uuid": "3.1.0",
    "web-push": "3.2.4",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2",
    "ws": "1.0.1",
    "xml2js": "^0.4.19"
  }
