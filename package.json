{
  "name": "Booklist",
  "main": "app.js",
  "engines": {
    "node": "8.4.0",
    "npm": "4.6.1"
  },
  "description": "A book management web app for exploring modern web technologies.  Think ToDo, but more realistic",
  "version": "0.1.0",
  "scripts": {
    "deploy": "npm run build && modulus deploy -p my-library",
    "build": "npm run react-redux-build",
    "react-redux-build": "cd react-redux && rm -rf dist && set NODE_ENV='production' && webpack -p",
    "start": "node app.js",
    "heroku-postbuild":
      "gulp transpile-all && cd react-redux && rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm i && webpack -p"
  },
