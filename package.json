{
  "name": "booklist",
  "main": "app.js",
  "engines": {
    "node": "8.11.1",
    "npm": "5.8.0"
  },
  "description": "A book management web app for exploring modern web technologies.  Think ToDo, but more realistic",
  "version": "0.1.0",
  "scripts": {
    "deploy": "npm run build && modulus deploy -p my-library",
    "build": "npm run react-redux-build",
    "react-redux-build": "cd react-redux && rm -rf dist && set NODE_ENV='production' && webpack",
    "start": "node app.js",
    "startw": "nodemon app.js",
    "heroku-postbuild": "cd react-redux && npm i && webpack"
  },
  "dependencies": {
    "@std/esm": "0.25.2",
    "apac": "3.0.2",
    "aws-sdk": "2.3.14",
    "blueimp-md5": "2.3.0",
    "body-parser": "1.13.2",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.0",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "easy-express-controllers": "1.0.2",
    "express": "4.x",
    "express-graphql": "0.6.11",
    "express-session": "1.12.1",
    "express-ws": "1.0.0",
    "glob": "7.0.3",
    "graphql": "0.11.7",
    "graphql-tools": "2.8.0",
    "jimp": "0.2.28",
    "mkdirp": "0.5.1",
    "moment": "^2.13.0",
    "mongo-graphql-starter": "^0.6.14",
    "mongodb": "2.2.34",
    "multer": "1.1.0",
    "nodemailer": "2.3.0",
    "nodemailer-smtp-transport": "2.4.1",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-remember-me": "0.0.1",
    "request": "2.83.0",
    "typescript": "2.7.2",
    "uuid": "3.1.0",
    "web-push": "3.2.4",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14",
    "ws": "1.0.1"
  }
}
