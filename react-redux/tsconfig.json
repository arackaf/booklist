{
  "compilerOptions": {
    "allowUnreachableCode": true,
    "target": "esnext",
    "baseUrl": "./",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "moduleResolution": "node",
    "module": "esNext",
    "experimentalDecorators": true,
    "lib": ["es2015", "es2017", "dom"],
    "paths": {
      "@reach/dialog": ["./typings/modules/@reach/dialog"]
    }
  },
  "include": ["./**/*.ts", "./**/*.tsx"],
  "exclude": ["node_modules"]
}
